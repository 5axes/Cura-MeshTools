#: MeshTools.py:244
#, python-format
msgctxt "@info:status"
msgid "Model %s is not watertight, and may not print properly."
msgstr ""
"Il modello %s non è \"waterthight\" e potrebbe non essere stampato "
"correttamente."

#: MeshTools.py:249
msgctxt "@info:status"
msgid "Check X-Ray View and repair the model before printing it."
msgstr "Controlla la vista a raggi X e ripara il modello prima di stamparlo."

#: MeshTools.py:250
msgctxt "@action:button"
msgid "Show X-Ray View"
msgstr "Mostra la vista a raggi X"

#: MeshTools.py:253
msgctxt "@info:status"
msgid "Repair the model before printing it."
msgstr "Ripara il modello prima di stamparlo."

#: MeshTools.py:291
msgctxt "@info:status"
msgid "Please select a single model first"
msgstr "Si prega di selezionare prima un singolo modello"

#: MeshTools.py:296 MeshTools.py:314
msgctxt "@info:status"
msgid "Please select one or more models first"
msgstr "Si prega di selezionare prima uno o più modelli"

#: MeshTools.py:325
msgctxt "@info:status"
msgid "Check summary:"
msgstr "Controlla il riepilogo:"

#: MeshTools.py:330
msgctxt "@info:status"
msgid "is watertight"
msgstr "è \"waterthight\""

#: MeshTools.py:332
msgctxt "@info:status"
msgid "is not watertight and may not print properly"
msgstr "non è \"waterthight\" a non può essere stampato correttamente"

#: MeshTools.py:334
#, python-brace-format
msgctxt "@info:status"
msgid "and consists of {body_count} submeshes"
msgstr "e consiste in {body_count} sottomaglie"

#: MeshTools.py:345
msgctxt "@info:status"
msgid "Analysis summary:"
msgstr "Riepilogo dell'analisi:"

#: MeshTools.py:349
#, python-format
msgctxt "@info:status"
msgid "%d vertices, %d faces"
msgstr "%d vertici, %d facce"

#: MeshTools.py:351
#, python-format
msgctxt "@info:status"
msgid "area: %d mm2, volume: %d mm3"
msgstr "area: %d mm2, volume: %d mm3"

#: MeshTools.py:369
msgctxt "@info:status"
msgid "The mesh needs more extensive repair to become watertight"
msgstr ""
"La maglia necessita di una riparazione più estensiva per diventare "
"\"watertight\""

#: MeshTools.py:390
msgctxt "@info:status"
msgid "Split result:"
msgstr "Risultato della divisione:"

#: MeshTools.py:396
#, python-format
msgctxt "@info:status"
msgid "was split in %d submeshes"
msgstr "è stato diviso in %d sottomaglie"

#: MeshTools.py:398
msgctxt "@info:status"
msgid "could not be split into submeshes"
msgstr "non può essere diviso in sottomaglie"

#: MeshTools.py:412
msgctxt "@info:status"
msgid "Replacing a group is not supported"
msgstr "La sostituzione del gruppo non è supportata"

#: MeshTools.py:433 MeshTools.py:438
msgctxt "@title:window"
msgid "Select Replacement Mesh File"
msgstr "Seleziona il file maglia sostitutivo"

#: MeshTools.py:487
msgctxt "@info:status"
msgid "Reloading a group is not supported"
msgstr "Il ricaricamento di un gruppo non è supportato"

#: MeshTools.py:494
msgctxt "@info:status"
msgid "No link to the original file was found"
msgstr "Nessun collegamento trovato con il file originale"

#: MeshTools.py:506
msgctxt "@info:status"
msgid "Failed to load mesh"
msgstr "Impossibile caricare la maglia"

#: MeshTools.py:513
msgctxt "@info:status"
msgid "Replacing meshes with a group of meshes is not supported"
msgstr "La sostituzione di maglie con un gruppo di maglie non è supportato"

